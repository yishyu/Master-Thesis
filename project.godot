; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="XRAttempt"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

XRToolsUserSettings="*res://addons/godot-xr-tools/user_settings/user_settings.gd"
Globals="*res://scripts/globals.gd"

[debug]

gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_call_argument=1
gdscript/warnings/return_value_discarded=1

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-sqlite/plugin.cfg", "res://addons/godot-xr-tools/plugin.cfg")

[file_customization]

folder_colors={
"res://scenes/icons/": "blue",
"res://scenes/instances/": "purple",
"res://scenes/rooms/": "orange",
"res://scenes/ui/": "teal"
}

[input]

forward={
"deadzone": 0.5,
"events": []
}
backward={
"deadzone": 0.5,
"events": []
}
left={
"deadzone": 0.5,
"events": []
}
right={
"deadzone": 0.5,
"events": []
}

[layer_names]

3d_physics/layer_1="Basic Physics"
3d_physics/layer_17="Grab Layer"
3d_physics/layer_18="AR Switch Layer"
3d_physics/layer_21="Pointer surfaces"
3d_physics/layer_22="Pointer Buttons"
3d_physics/layer_23="Pointer Ball"

[rendering]

textures/vram_compression/import_etc2_astc=true

[xr]

openxr/enabled=true
openxr/environment_blend_mode=2
openxr/foveation_level=3
openxr/foveation_dynamic=true
shaders/enabled=true
